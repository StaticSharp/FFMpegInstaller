<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>FFMpegInstaller.Windows.x64</id>
    <version>0.1.0</version>
    <license type="expression">MIT AND GPL-3.0-only</license>
    <authors>StaticSharp</authors>
    <description>Package Description</description>
    <dependencies>
      <group targetFramework="netstandard2.1" />
    </dependencies>
    <contentFiles>
        <files include="any\any\ffmpeg\**\*.*" buildAction="None" copyToOutput ="True" />   
    </contentFiles>
  </metadata>
    <files>
        <file src="bin\Release\netstandard2.1\**\*.dll" target="lib\netstandard2.1" />
        <file src="ffmpeg\**\*" target="contentFiles\any\any\ffmpeg" />
  </files>
</package>